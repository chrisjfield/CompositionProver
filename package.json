    "lint": "npx eslint . --ext .ts,.tsx"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "airbnb-typescript"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "react/react-in-jsx-scope": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
